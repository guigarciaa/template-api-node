{
    "exclude": [
      "node_modules"
    ],
    "compilerOptions": {
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "lib": [
        "dom",
        "esnext"
      ],
      "module": "commonjs",
      "strict": true,
      "target": "ESNEXT",
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "allowJs": false,
      "sourceMap": true,
      "outDir": "./dist",
      "baseUrl": "./",
      "removeComments": true,
      "moduleResolution": "node",
      "noImplicitAny": false,
    }
  }